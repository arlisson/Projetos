.data
	msg_1: .asciz "Informe a sua idade: "	
	cp: .asciz "\nCriança"
	adolescente: .asciz "\nAdolescente"
	adulto: .asciz "\nAdulto"
	idoso: .asciz "\nIdoso"
	
.text
.global main
main:

	li s2 12
	li s3 18
	li s4 59
	li s5 60

	li a7 4
	la a0 msg_1
	ecall
	
	li a7 5
	ecall
	mv t0 a0
	
	ble t0 s2 crianca
	ble t0 s3 adol
	ble t0 s4 adul
	ble s5 t0 ido
		
	j sair
	
crianca:
	
	li a7 4
	la a0 cp
	ecall
	j sair

adol:
	
	li a7 4
	la a0 adolescente
	ecall
	j sair

adul:
	
	li a7 4
	la a0 adulto
	ecall
	j sair

ido:
	
	li a7 4
	la a0 idoso
	ecall
	j sair
	
sair:
	li a7 10	
	ecall
	