.data

	a: .asciz "Informe a: "
	b_: .asciz "Informe b: "
	c: .asciz "Informe c: "
	distintas: .asciz "Raízes reais e distintas!"
	iguais: .asciz "Raízes reais e iguais!"
	complexas: .asciz "Rízes Complexas!"
	
.text
.global main
main:
	flw fs0 -4
	
	li a7 4
	la a0 a
	ecall
	
	li a7 6
	ecall
	mv t0 a0
	
	li a7 4
	la a0 b_
	ecall
	
	li a7 6
	ecall
	mv t1 a0
	
	li a7 4
	la a0 c
	ecall
	
	li a7 6
	ecall
	mv t2 a0
	
	fmv.s.x ft0 t0
	fmv.s.x ft1 t1
	fmv.s.x ft2 t2
	
	
	j calcula
	
calcula:
	#ft0 = a
	fmul.s ft1 ft1 ft1 #t1 = b^2
	fmul.s ft3 ft0 ft2 #t3 = a*c
	fmul.s ft4 ft3 fs0
	fadd.s ft5 ft4 ft1
	
	#fsqrt.d ft6 ft5 
	
	
	li a7 2
	fmv.x.w a0 ft5
	ecall
	
	j fim



fim:
	li a7 10
	ecall
